{
  "name": "string-guard",
  "railspec": {
     "outputSchema": {
        "schema": {
            "element": {
               "type": "string",
               "name": "pet_name",
               "description": "Generate a pet name.",
               "onFail": "fix"
            },
            "formatters": ["hub://guardrails/valid_length: 1 10"]
        }
     },
     "prompt": "Generate a random pet name between 1 and 10 characters.  Return only the generated pet name and nothing else. Don't talk, just go.",
     "version": "0.1"
  },
  "numReasks": 1
}
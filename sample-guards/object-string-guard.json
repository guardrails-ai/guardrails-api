{
  "name": "object-string-guard",
  "railspec": {
     "outputSchema": {
        "schema": {
            "pet_name": {
               "element": {
                  "type": "string",
                  "name": "pet_name",
                  "description": "Generate a pet name.",
                  "onFail": "fix"
               },
               "formatters": ["length: 1 10"]
            }
        }
     },
     "prompt": "Generate a random pet name between 1 and 10 characters.  Each row of the dataset should be valid.\n\n${gr.complete_json_suffix}",
     "version": "0.1"
  },
  "numReasks": 1
}